package ejerT1;

import java.io.File;
 

public class ejer7 {
	public static void main(String[] args) {

		File directorio = new File(args[0]);
		for (int i =1;i<args.length;i++) {
		String[] listaArchivos=directorio.list(new FiltroExtension(args[i]));
		System.out.println(listaArchivos);
		}

	}

	
}