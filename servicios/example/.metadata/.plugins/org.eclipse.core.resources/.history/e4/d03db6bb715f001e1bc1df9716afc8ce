package example;

import java.util.Scanner;

public class Ejer8 {
	public static void main(String[] args) {
		Scanner teclado = new Scanner(System.in);

		System.out.println("Posa un numero del 1 al 12: ");
		int numero1 = teclado.nextInt();
		 System.out.println("Hola: " + numero1);
		teclado.close();
		
	}
}